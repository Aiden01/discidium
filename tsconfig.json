{
    "compilerOptions": {
        "lib": ["es5", "es7"],
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    }
}